{
  "routes": [
    {
      "route": "/build/*.gz",
      "headers": {
        "Content-Encoding": "gzip"
      }
    },
    {
      "route": "/build/*.js",
      "headers": {
        "Content-Type": "application/javascript"
      }
    },
    {
      "route": "/build/*.data",
      "headers": {
        "Content-Type": "application/octet-stream"
      }
    },
    {
      "route": "/build/*.wasm",
      "headers": {
        "Content-Type": "application/wasm"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  }
}
